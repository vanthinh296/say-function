#. Translators: Title of the settings panel in NVDA preferences
msgid "Say Function"
msgstr "Đọc chức năng"

#. Translators: Label for checkbox in settings
msgid "Play different sound effects when performing keyboard shortcuts"
msgstr "Phát các âm thanh khác nhau khi thực hiện phím tắt"

#. Translators: Note shown when sound libraries are not available
msgid "Note: Sound libraries not available. Only tone beeps will work."
msgstr "Lưu ý: Thư viện âm thanh không khả dụng. Chỉ có tiếng bíp sẽ hoạt động."

#. Translators: Message announced when copy command is performed
msgid "Copy"
msgstr "Sao chép"

#. Translators: Message announced when paste command is performed
msgid "Pasted"
msgstr "Đã dán"

#. Translators: Message announced when cut command is performed
msgid "Cut"
msgstr "Cắt"

#. Translators: Message announced when undo command is performed
msgid "Undo"
msgstr "Hoàn tác"

#. Translators: Message announced when redo command is performed
msgid "Redo"
msgstr "Khôi phục"

#. Translators: Message announced when select all command is performed
msgid "Select all"
msgstr "Chọn tất cả"
